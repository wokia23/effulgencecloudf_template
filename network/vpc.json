{
    "AWSTemplateFormatVersion": "2010-09-09",
        "Description": "This code deploys network stack for effulgence application",
        
        "Resources": {
           "myVPC" : {
               "Type" : "AWS::EC2::VPC",
               "Properties" : {
                   "CidrBlock" : "10.0.0.0/16",
                   "EnableDnsSupport" : "true",
                   "EnableDnsHostnames" : "true",
                   "Tags" : [ 
                       {"Key" : "stack", "Value" : "production"} 
                   ]
               }
           }
       }
    }
         